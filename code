
public class Main {
    public static void main(String[] args) {
        int number = 5;
        System.out.println(number);

        System.out.println(factorial(number));
    }

    public static int factorial(int input) {
        if (input > 1) {
            return input * factorial(input - 1);
        } else {
            return 1;
        }
    }
}
